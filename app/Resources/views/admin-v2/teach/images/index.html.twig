{% extends 'admin-v2/layout.html.twig' %}
{% set menu = 'admin_v2_images_manage' %}
{% set script_controller = 'course/manage' %}
{% do script(['app/js/images/manage/index.js']) %}

{% block main %}

  <table class="table table-striped table-hover" id="images-table" style="word-break:break-all;">
    <thead>
    <tr>
      <th>编号</th>
      <th width="20%">镜像名称</th>
      
      <th>状态</th>
      <th>操作</th>
    </tr>
    </thead>
    <tbody>
    {% for image in lists %}
      {% include 'admin-v2/teach/images/tr.html.twig' %}
    {% else %}
      <tr>
        <td colspan="20">
          <div class="empty">{{'admin.course_manage.manage.empty'|trans}}</div>
        </td>
      </tr>
    {% endfor %}
    </tbody>
  </table>
  {{ admin_macro.paginator(paginator) }}

{% endblock %}