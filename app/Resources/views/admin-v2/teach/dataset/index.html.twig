{% extends 'admin-v2/layout.html.twig' %}
{% set menu = 'admin_v2_dataset_manage' %}

{% block main %}

  <table class="table table-striped table-hover" id="dataset-table" style="word-break:break-all;">
    <thead>
    <tr>
      <th>编号</th>
      <th width="20%">数据集名称</th>
      <th>创建时间</th>
      <th>操作</th>
    </tr>
    </thead>
    <tbody>
    {% for image in lists %}
      {% include 'admin-v2/teach/dataset/tr.html.twig' %}
    {% else %}
      <tr>
        <td colspan="20">
          <div class="empty">数据集为空</div>
        </td>
      </tr>
    {% endfor %}
    </tbody>
  </table>
  {{ admin_macro.paginator(paginator) }}

{% endblock %}