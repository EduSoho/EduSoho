{% extends 'old-bootstrap-modal-layout.html.twig' %}
{% set modal_class = 'modal-lg' %}
{% block title %}{{'open_live.plugin.admin.sms_reached_student.list'|trans}}{% endblock %}
{% block body %}

  <form id="user-search-form" class="form-inline well well-sm" action="{{ path('admin_v2_open_live_sms_reached_student_ajax_search',{'id':roomId}) }}" method="post" novalidate>
    <div class="form-group">
      <input type="text" id="smsReachedListKeyword" name="keyword" class="form-control" value="{{ app.request.get('keyword') }}"
             placeholder="{{ 'open_live.plugin.admin.sms_reached_student.input.mobile_require'|trans }}">
    </div>

    <a class="btn btn-primary" id="search">{{ 'open_live.plugin.admin.list.data_search'|trans }}</a>
    {% include 'OpenLivePlugin::admin/sms-reached-student/open-live-sms-reached-student-export-btn.html.twig' %}
  </form>

  <div class="data-list">
    {% include 'OpenLivePlugin::admin/sms-reached-student/open-live-sms-reached-student-tr.html.twig' with {smsReachedStudents: smsReachedStudents} %}
  </div>

{% endblock %}
{% block footer %}
  <button type="button" class="btn btn-link pull-right" data-dismiss="modal">{{ 'form.btn.close'|trans }}</button>
  <script>app.load('openliveplugin/controller/open-live/open-live-sms-reached-list');</script>
{% endblock %}